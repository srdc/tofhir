{
  "id": "test-mapping-job",
  "sourceSettings": {
    "source": {
      "jsonClass": "FileSystemSourceSettings",
      "name": "test-source",
      "sourceUri": "https://aiccelerate.eu/data-integration-suite/test-data",
      "dataFolderPath": "./tofhir-engine/src/test/resources/test-data"
    }
  },
  "sinkSettings": {
    "jsonClass": "FhirRepositorySinkSettings",
    "fhirRepoUrl": "http://localhost:8081/fhir",
    "writeErrorHandling": "halt"
  },
  "mappings": [
    {
      "mappingRef": "https://aiccelerate.eu/fhir/mappings/patient-mapping",
      "sourceContext": {
        "source": {
          "jsonClass": "FileSystemSource",
          "path": "patients.csv"
        }
      }
    },
    {
      "mappingRef": "https://aiccelerate.eu/fhir/mappings/other-observation-mapping",
      "sourceContext": {
        "source": {
          "jsonClass": "FileSystemSource",
          "path": "other-observations.csv"
        }
      }
    }
  ],
  "mappingErrorHandling": "halt"
}
